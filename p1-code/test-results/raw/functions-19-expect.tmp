    cmdcol_t cmdcol_actual = {};
    cmdcol_t *cmdcol = &cmdcol_actual;
    for(int i=0; children[i][0] != NULL; i++){
      cmd_t *cmd = cmd_new(children[i]);
      cmdcol_add(cmdcol, cmd);
    }
    cmdcol_print(cmdcol);
    cmdcol_freeall(cmdcol);
}
